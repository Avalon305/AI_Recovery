        <Button Content="测试AES" HorizontalAlignment="Left" Margin="470,45,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_4"/>
        <Button Content="测试数据库" HorizontalAlignment="Left" Margin="530,150,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_5"/>
        <Button Content="测试按主键更新" HorizontalAlignment="Left" Margin="202,150,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_6"/>
        
        <ComboBox x:Name="TestCombox" Loaded="TestCombox_Loaded" SelectionChanged="TestCombox_SelectionChanged"
