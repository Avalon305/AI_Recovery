            </Grid>
        </Grid>
        <!-- 四个按钮排放位置-->
        <Border CornerRadius="5,5,0,0" Background="White" HorizontalAlignment="Right" Width="335" Height="29" VerticalAlignment="Bottom" Padding="0,2,0,2" Margin="0,0,9.6,0">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" >

                <Button Width="140"  Click="ButtonBase_OnClick"  Content="保存" Margin="2,0,0,-0.2" Style="{StaticResource Btn}" RenderTransformOrigin="0.5,0.504" />
                <Button Width="140"     Content="取消"  Style="{StaticResource Btn}" Margin="2,0,2,-0.2" Click="Cancel"/>
            </StackPanel>
        </Border>
    </StackPanel>
</Window>
