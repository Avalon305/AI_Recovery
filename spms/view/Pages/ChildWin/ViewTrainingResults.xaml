        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:spms.view.Pages.ChildWin"
        mc:Ignorable="d"
        PreviewKeyDown="key_dowm"
        Icon="/view/Images/icon2.png"
        Title="{DynamicResource Title.Title}"
        ResizeMode="CanMinimize"  SizeToContent="WidthAndHeight" 
        Loaded="Window_Loaded">
    <Viewbox Name="viewbox" Loaded="viewbox_load" Stretch="Uniform">
        <StackPanel>
            <!--Border 使用者一览表以及按钮  开始-->
            <Border Height="40" CornerRadius="5,5,0,0"  Margin="5,0,5,0">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                        <GradientStop Color="Silver" Offset="0.1"/>
                        <GradientStop Color="Gray" Offset="0.9"/>
                    </LinearGradientBrush>

                </Border.Background>
                <Grid>
                    <Label VerticalAlignment="Bottom" Margin="10,0,0,0" Foreground="White" FontSize="20" Height="38" Content="{DynamicResource TrainingResultView.TrainingResult}"></Label>
                </Grid>
            </Border>
            <!--Border 训练结果以及按钮  结束-->

            <!--StackPanel 信息以及日期-->
            <StackPanel  Orientation="Horizontal" Margin="5,0,5,0">
                <!--Grid 使用者信息展示-->
                <Grid Height="30" Margin="0,0,18.6,0" Width="609">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="15*"/>
                        <ColumnDefinition Width="82*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Row="0" Grid.Column="0"  Content="{DynamicResource TrainingResultView.SubjectID}" Visibility="Collapsed" Margin="0,-6,34.4,15.6" Grid.RowSpan="2"/>
                    <Label Grid.Row="0"  Margin="65,-6,-0.2,15.6" Name="l2" Grid.ColumnSpan="2" Visibility="Collapsed" Grid.RowSpan="2"/>
                    <Label Grid.Column="0"  Content="{DynamicResource TrainingResultView.LastName}" VerticalAlignment="Center" Margin="0,3,22.4,1.4" Grid.RowSpan="2"/>
                    <Label  Margin="77,11,-0.2,9.4" Name="l1" Grid.ColumnSpan="2" Grid.RowSpan="2" VerticalAlignment="Center"/>
                </Grid>
            </StackPanel>
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                <Border Margin="5,0,5,0" Background="LightGray" Height="25">
                    <Label  Content="{DynamicResource TrainingResultView.Date}"  FontSize="12" Margin="0,0,0,0.2"/>
                </Border>
                <!--日历-->
                <Border Margin="10" Width="153" Height="25" BorderThickness="1" BorderBrush="Gray" >
                    <StackPanel Orientation="Horizontal" >
                        <Label x:Name="da" Width="125" VerticalContentAlignment="Center" IsEnabled="False" Height="23" BorderBrush="Gray" BorderThickness="0"></Label>
                        <Image Source="/view/Images/date.png" Margin="5,5,0,5"></Image>
                    </StackPanel>
                </Border>
            </StackPanel>
